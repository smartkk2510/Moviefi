<h2>Recently Added</h2>
<div>
    <% movies.forEach((movie) => { %>
        <a href="/movies/<%= movie.id %>">
        <div style="display: inline-block;">
            <img style="margin:3px 10px 0;" src="<%= movie.posterImagePath %>" height="150" width="100"  alt="img">
            <h3 style="margin:0 10px 3px;"> <%=movie.name %></h3>
        </div>
    </a>
       
    <% }); %>  
</div>